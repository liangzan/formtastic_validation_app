<table>
  <thead>
    <tr>
      <td>Name</td>
      <td>Email</td>
      <td>Mobile</td>
      <td></td>
      <td></td>
      <td></td>
    </tr>
  </thead>

  <tbody>
    <%- @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.mobile %></td>
        <td><%= link_to('Show', user_path(user)) %></td>
        <td><%= link_to('Edit', edit_user_path(user)) %></td>
        <td><%= link_to('Destroy', user_path(user), :method => :delete) %></td>
      </tr>
    <%- end %>
  </tbody>
</table>

<%= link_to "Create user", new_user_path %>
